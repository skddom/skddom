<script>
jQuery(document).on('click','#feedback- form input[type="submit"]',function() { 
var m = jQuery(this).closest('form'); 
var fio = m.find('input[name="f_Name"]').val(); 
var mail = m.find('input[name="f_Email"]').val();
var theme = m.find('input[name="f_Subject"]').val();
var comment = m.find('textarea[name="f_Text"]').val();
var sub = 'Обратная связь';
var ct_site_id = '';
var sessionId = '';
if(!!window.call_value_1){ct_site_id = '20774'; sessionId = window.call_value_1;}
if(!!window.call_value_2){ct_site_id = '20835'; sessionId = window.call_value_2;}
if(!!window.call_value_3){ct_site_id = '20836'; sessionId = window.call_value_3;}
var ct_data = {             
fio: fio,
email: mail,
subject: sub,
sessionId: sessionId 
};
if (!!mail && !!fio && !!theme && !!comment){
jQuery.ajax({  
  url: 'https://api-node7.calltouch.ru/calls-service/RestAPI/'+ct_site_id+'/requests/orders/register/',      
  dataType: 'json',         
  type: 'POST',          
  data: ct_data,
  async: false
}); 
}
});
</script>
